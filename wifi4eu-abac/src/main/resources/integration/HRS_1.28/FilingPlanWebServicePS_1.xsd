<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://ec.europa.eu/sg/hrs/types" elementFormDefault="qualified" xmlns:tns="http://ec.europa.eu/sg/hrs/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2001/XMLSchema">
  <!--Element definitions-->
  <element name="header" type="tns:Header">
    <annotation>
      <documentation>The header allows HRS to authenticate and authorize users and applications calling the web services</documentation>
    </annotation>
  </element>
  <element name="hrsFault" type="tns:HrsFault"/>
  <simpleType name="ContentIdType">
    <annotation>
      <documentation>An unique Content id string which is obtained while uploading the content.</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="ci-[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
    </restriction>
  </simpleType>
  <xsd:simpleType name="JobUserName">
    <annotation>
      <documentation>The user name/id a job account can use to authenticate</documentation>
    </annotation>
    <restriction base="string">
      <pattern value="je\_[a-z0-9]{4,17}"/>
    </restriction>
  </xsd:simpleType>
  <!--Type definitions-->
  <complexType name="HrsFault">
    <sequence>
      <element name="id" type="string">
        <annotation>
          <documentation>An ID that identifies the fault. This ID will be unique for each fault.</documentation>
        </annotation>
      </element>
      <element name="code">
        <annotation>
          <documentation>The code associated with the fault.</documentation>
        </annotation>
        <simpleType>
          <restriction base="xsd:string">
            <enumeration value="HRS_INTERNAL_FAULT">
              <annotation>
                <documentation>Indicates an HRS internal fault, please report this issue to
                                    the HRS support team</documentation>
              </annotation>
            </enumeration>
            <enumeration value="HRS_NO_OBJECT_FAULT">
              <annotation>
                <documentation>Indicates that the request object (e.g. document, file, ...)
                                    was not found. It's possible that it does not exist or that it's not visible (no
                                    access rights) to the operation user</documentation>
              </annotation>
            </enumeration>
            <enumeration value="HRS_LOCK_FAULT">
              <annotation>
                <documentation>Indicates that an object was locked while trying to access it.
                                    This could indicate that there is concurrent access to a given object or that
                                    an attachment of a document has been checked out.</documentation>
              </annotation>
            </enumeration>
            <enumeration value="HRS_PERMISSION_FAULT">
              <annotation>
                <documentation>Indicates that the operating user does not have the
                                    permission to perform a given action (e.g. not allowed to register a document)</documentation>
              </annotation>
            </enumeration>
            <enumeration value="HRS_BUSINESS_FAULT">
              <annotation>
                <documentation>Indicates a generic, uncategorized business fault due to a
                                    business requirement not met.</documentation>
              </annotation>
            </enumeration>
            <enumeration value="HRS_CONCURRENT_MODIFICATION_FAULT">
              <annotation>
                <documentation>Indicates a concurrent modification of an object where optimistic locking is
                                    applied. For example concurrently linking a given Document with other documents.</documentation>
              </annotation>
            </enumeration>
          </restriction>
        </simpleType>
      </element>
      <element name="key" type="string" minOccurs="0">
        <annotation>
          <documentation>The key associated with the exception. HRS uses this key internally for
                        retrieving messages associated with exceptions. Clients could also use this to have
                        fine grained error messages.</documentation>
        </annotation>
      </element>
      <element name="stackTrace" minOccurs="0">
        <annotation>
          <documentation>String representation of the exception stacktrace. This element is mainly
                        for HRS internal usage and debugging purposes.</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="line" type="string" minOccurs="0" maxOccurs="unbounded"/>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <!--Type definitions-->
  <complexType name="Header">
    <sequence>
      <element name="userName" type="string">
        <annotation>
          <documentation>ECAS user name of the user / job that authenticates with HRS.

                        For job users, you should use the ECAS job user name (e.g. j00x000) and not
                        the internal job user name (e.g. je_xxxx).</documentation>
        </annotation>
      </element>
      <element name="mappedJobUser" type="tns:JobUserName" minOccurs="0">
        <annotation>
          <documentation>In order to have a single ECAS user mapped to multiple Hermes Job users, this element depicts
                         the mapped Hermes job user.
                         To facilitate the management of job users and certificates, we allow a single
                         ECAS job user to be mapped to multiple Hermes job users. e.g if a single application is possessing all the
                         Hermes jobs.
                         Element 'userName' will have ECAS user and this element will have corresponding mapped Hermes
                         job user and request will be considered as triggered by this mapped Hermes job user.</documentation>
        </annotation>
      </element>
      <element name="onBehalfOf" type="string" minOccurs="0">
        <annotation>
          <documentation>User name of the user on which behalf the operation should be performed.
                        Currently only delegations from normal users to virtual entities are supported.</documentation>
        </annotation>
      </element>
      <element name="ticket" type="string">
        <annotation>
          <documentation>ECAS ticket for the HRS service used for authentication. The ticket should
                        belong to the user provided in userName.</documentation>
        </annotation>
      </element>
      <element name="applicationId" type="string">
        <annotation>
          <documentation>Application ID associated to the an application invoking HRS.
                        The application ID is used to identify and authorize the calling application</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="OperationResponse">
    <annotation>
      <documentation>Generic response type for operations that do not require a specific result</documentation>
    </annotation>
    <sequence>
      <element name="success" type="boolean" default="true">
        <annotation>
          <documentation>Status of the operation invocation</documentation>
        </annotation>
      </element>
      <element name="message" type="string" minOccurs="0">
        <annotation>
          <documentation>Message providing extra information regarding the invocation</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="DocumentRetrievalOptions">
    <annotation>
      <documentation>Options allowing to precise the parts of a document we need to retrieve. It allows client applications to define which metadata to retrieve for a document.</documentation>
    </annotation>
    <sequence>
      <choice>
        <element name="includeItemsMetadata" type="boolean" minOccurs="0" default="false">
          <annotation>
            <documentation>Specifies that the document search response will contain the metadata of the latest attachments</documentation>
          </annotation>
        </element>
        <element name="includeItemsAndVersionsMetadata" type="boolean" minOccurs="0" default="false">
          <annotation>
            <documentation>Specifies that the document search response will contain the metadata of the latest attachments and their previous versions.
                            If the document is registered, only the creator and e-Signatory assignees can see the versions metadata.</documentation>
          </annotation>
        </element>
      </choice>
      <choice>
        <element name="includeSendersAndRecipientsMetadata" type="boolean" minOccurs="0" default="false">
          <annotation>
            <documentation>Specifies that the document search response will contain the metadata of senders/receivers.
                            Equivalent to specifying both includeSenders and includeRecipients.</documentation>
          </annotation>
        </element>
        <sequence>
          <element name="includeSenders" type="boolean" minOccurs="0" default="false">
            <annotation>
              <documentation>Specifies that the document search response will contain the metadata of senders</documentation>
            </annotation>
          </element>
          <element name="includeRecipients" type="boolean" minOccurs="0" default="false">
            <annotation>
              <documentation>Specifies that the document search response will contain the metadata of recipients</documentation>
            </annotation>
          </element>
        </sequence>
      </choice>
      <element name="includeSendersRecipientsValidationForRegistration" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that a flag will be included indicating that the sender/recipient is valid to be used in a document
                        registration. The flag will only be present for the senders/recipients of non-registered documents and
                        will only apply if the retrieval option includeSenders, includeRecipients or includeSendersAndRecipientsMetadata
                        was set.
                        This flag for senders/recipients can be useful if you create a document at a given moment and you register
                        it at a later moment in time. During the registration process there's a validation that the senders and
                        recipients are still valid according their current assignment. For internal entities it could be that they
                        changed service, DG, ... or that they left the EC. In those cases they are considered invalid. External
                        entities could have been deleted (e.g. because of a consolidation).</documentation>
        </annotation>
      </element>
      <element name="includeFilingInfo" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>It specifies that the document search response will contain filing information</documentation>
        </annotation>
      </element>
      <element name="includeLinks" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>It specifies that the document search response will contain the links of the document</documentation>
        </annotation>
      </element>
      <element name="includeAssignmentWorkflow" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>It specifies that the document search response should contain the Assignment workflow if existing</documentation>
        </annotation>
      </element>
      <element name="includeSignatoryWorkflow" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>It specifies that the document search response should contain the Signatory Paper or e-Signatory workflow if existing</documentation>
        </annotation>
      </element>
      <element name="limitTasksToCurrentUser" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>When retrieving the workflow (Assignment or Signatory) limit returned tasks to those
                        assigned to the current user.
                        This can be used as a performance optimization, since the Assignment workflow of a document
                        (and to a lesser extent the Signatory workflow) can include hundreds of tasks in Production.</documentation>
        </annotation>
      </element>
      <element name="limitTasksToActiveTasks" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>When retrieving the workflow of a document (Assignment or Signatory) limit
                        returned tasks to those that are active.
                        This can be used as a performance optimization, since the Assignment workflow of a document
                        (and to a lesser extent the Signatory workflow) can include hundreds of tasks in Production.</documentation>
        </annotation>
      </element>
      <element name="includeProcedure" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>It specifies that the document search response should include the procedure (if any) linked to the document</documentation>
        </annotation>
      </element>
      <element name="includeSignedBy" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include the users that have signed the document (applies only to registered documents).</documentation>
        </annotation>
      </element>
      <element name="includeExternalizationDetails" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include information about the externalization process of the document to the ERS repository.</documentation>
        </annotation>
      </element>
      <element name="includeSpecificMetadata" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include (business) specific metadata.</documentation>
        </annotation>
      </element>
      <element name="includeSpecificMetadataItemsMetadata" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include the metadata (business) specific metadata items/attachments.</documentation>
        </annotation>
      </element>
      <element name="includeExternalSignatures" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include information related to external signatures.</documentation>
        </annotation>
      </element>
      <element name="includeAccessibilityInfo" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Include additional information related to accessibility (e.g. in Hermes, A-REP, ...) of documents.
                        In case the document is not accessible to the current user, next to this accessibility information,
                        limited (non-sensitive) metadata for the documents will be returned.
                        Can only be used in combination with certain search expressions like registrationNumber = 'Ares(2017)123'
                        and is not applicable for the index server searches.</documentation>
        </annotation>
      </element>
      <element name="includeSignatoryWorkflowSupportingItems" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>A flag to indicate if supporting-items need to be retrieved or not, while fetching document information.
                        If there are any supporting-item(s) associated with document's e-signatory workflow, then these item(s) can be
                        retrieved by setting this flag to true.</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="SortOptions">
    <annotation>
      <documentation>Options allowing to sort the result of a search</documentation>
    </annotation>
    <sequence>
      <element name="sortBy" minOccurs="1" maxOccurs="2">
        <complexType>
          <sequence>
            <element name="fieldName" type="string">
              <annotation>
                <documentation>The name of the field to sort on</documentation>
              </annotation>
            </element>
            <element name="order">
              <annotation>
                <documentation>The sort order</documentation>
              </annotation>
              <simpleType>
                <restriction base="xsd:string">
                  <enumeration value="ASC">
                    <annotation>
                      <documentation>ascending order</documentation>
                    </annotation>
                  </enumeration>
                  <enumeration value="DESC">
                    <annotation>
                      <documentation>descending order</documentation>
                    </annotation>
                  </enumeration>
                </restriction>
              </simpleType>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="FileRetrievalOptions">
    <annotation>
      <documentation>Options allowing to precise the information of a file we need to retrieve. If less information is required, the operation can result in better response times.</documentation>
    </annotation>
    <sequence>
      <element name="includeDocumentFilingAllowed" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain information if the current user can file a document into it</documentation>
        </annotation>
      </element>
      <element name="includeUserFileRole" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain information about the current user's role on the file</documentation>
        </annotation>
      </element>
      <element name="includeHasSubfiles" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain information whether it has subfiles to which the current user has access</documentation>
        </annotation>
      </element>
      <element name="includeRoles" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain the roles (file readers, file users, file editors) of the file</documentation>
        </annotation>
      </element>
      <element name="includeCategory" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain the linked category (if there's one linked)</documentation>
        </annotation>
      </element>
      <element name="includeAssociatedLeadDepartments" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain the associated lead departments (if any)</documentation>
        </annotation>
      </element>
      <element name="includeSystemContainsDocuments" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain information whether it has documents or not, regardless of user access</documentation>
        </annotation>
      </element>
      <element name="includeSystemHasSubfiles" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain information whether it has subfiles or not, regardless of user access</documentation>
        </annotation>
      </element>
      <element name="includeFullPreservationMetadata" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain full preservation metadata information (information like shelf, cupboard and office
                        is always returned, if available)</documentation>
        </annotation>
      </element>
      <element name="includeTransparencyMetadata" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies that the file will contain transparency metadata information</documentation>
        </annotation>
      </element>
      <element name="includeSystemDocumentCount" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies the count of documents which are visible to the logged in user in a given file</documentation>
        </annotation>
      </element>
      <element name="includeAccessibleDocumentCount" type="boolean" minOccurs="0" default="false">
        <annotation>
          <documentation>Specifies the total number of documents which are filed in a given file</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="File">
    <annotation>
      <documentation>A file (dossier)</documentation>
    </annotation>
    <sequence>
      <element name="fileId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID that uniquely identifies the file</documentation>
        </annotation>
      </element>
      <element name="isSubFile" type="xsd:boolean">
        <annotation>
          <documentation>It indicates if the file is a subfile</documentation>
        </annotation>
      </element>
      <element name="isDocumentFilingAllowed" type="xsd:boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates if a user has rights to file documents into this file.
                    If the file is ACTIVE, not special and the user is file user (directly or
                    indirectly), the user has the right the filing right.
                    Note that the user must also have sufficient permissions on the document(s) (s)he
                    wants to file into this file.</documentation>
        </annotation>
      </element>
      <element name="path" type="xsd:string">
        <annotation>
          <documentation>Logical path to the file</documentation>
        </annotation>
      </element>
      <element name="chefDeFile" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Name of 'main lead department' / 'chef de file'</documentation>
        </annotation>
      </element>
      <element name="associatedLeadDepartments" type="tns:AssociatedLeadDepartments" minOccurs="0">
        <annotation>
          <documentation>The list of associated lead departments</documentation>
        </annotation>
      </element>
      <element name="headingCode" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>The file's heading code</documentation>
        </annotation>
      </element>
      <element name="fileCode" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>The file's code</documentation>
        </annotation>
      </element>
      <element name="specificCode" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>The file's specific code</documentation>
        </annotation>
      </element>
      <element name="status" type="tns:FileStatus" minOccurs="0">
        <annotation>
          <documentation>The file's status</documentation>
        </annotation>
      </element>
      <element name="parentId" type="tns:ObjectId" minOccurs="0">
        <annotation>
          <documentation>Internal repository ID of the parent file or heading</documentation>
        </annotation>
      </element>
      <element name="englishName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the file in English</documentation>
        </annotation>
      </element>
      <element name="frenchName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the file in French</documentation>
        </annotation>
      </element>
      <element name="germanName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the file in German</documentation>
        </annotation>
      </element>
      <element name="userFileRoles" minOccurs="0">
        <annotation>
          <documentation>The roles the operating user has on the file</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="userFileRole" minOccurs="0" maxOccurs="unbounded">
              <simpleType>
                <restriction base="xsd:string">
                  <enumeration value="NOMCOM_MANAGER"/>
                  <enumeration value="FILE_EDITOR"/>
                  <enumeration value="FILE_USER"/>
                  <enumeration value="FILE_READER"/>
                </restriction>
              </simpleType>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="hasSubFiles" type="xsd:boolean" minOccurs="0">
        <annotation>
          <documentation>It indicates if the file has sub files, to which the current user has access</documentation>
        </annotation>
      </element>
      <element name="readers" minOccurs="0">
        <annotation>
          <documentation>Collection of file readers</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="reader" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the file reader</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="users" minOccurs="0">
        <annotation>
          <documentation>Collection of file readers</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="user" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the file user</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="editors" minOccurs="0">
        <annotation>
          <documentation>Collection of file readers</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="editor" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the file editor</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="englishComments" type="string" minOccurs="0">
        <annotation>
          <documentation>Comments in English</documentation>
        </annotation>
      </element>
      <element name="frenchComments" type="string" minOccurs="0">
        <annotation>
          <documentation>Comments in French</documentation>
        </annotation>
      </element>
      <element name="germanComments" type="string" minOccurs="0">
        <annotation>
          <documentation>Comments in German</documentation>
        </annotation>
      </element>
      <element name="deskOfficer" type="string" minOccurs="0">
        <annotation>
          <documentation>The desk officer's user name</documentation>
        </annotation>
      </element>
      <element name="visibility" type="tns:FileVisibilityType" minOccurs="0">
        <annotation>
          <documentation>Indicates the visibility level of the File (and its contents)</documentation>
        </annotation>
      </element>
      <element name="special" type="boolean" minOccurs="0">
        <annotation>
          <documentation>True if this file is special - PMO Document ('blue'). False or missing otherwise.</documentation>
        </annotation>
      </element>
      <element name="creationDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was created</documentation>
        </annotation>
      </element>
      <element name="activationDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was activated</documentation>
        </annotation>
      </element>
      <element name="closingDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was closed</documentation>
        </annotation>
      </element>
      <element name="transferDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was transfered</documentation>
        </annotation>
      </element>
      <element name="destroyDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was destroyed</documentation>
        </annotation>
      </element>
      <element name="restoreDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Lifecyle information: date when the file was restored</documentation>
        </annotation>
      </element>
      <element name="category" type="tns:Category" minOccurs="0">
        <annotation>
          <documentation>The category linked to the file</documentation>
        </annotation>
      </element>
      <element name="systemHasSubFiles" type="xsd:boolean" minOccurs="0">
        <annotation>
          <documentation>It indicates if the file has sub files, regardless of user access</documentation>
        </annotation>
      </element>
      <element name="systemContainsDocuments" type="xsd:boolean" minOccurs="0">
        <annotation>
          <documentation>It indicates if the file has documents, regardless of user access</documentation>
        </annotation>
      </element>
      <element name="preservationMetadata" type="tns:FilePreservationMetadata" minOccurs="0">
        <annotation>
          <documentation>Information about the preservation metadata associated to the file</documentation>
        </annotation>
      </element>
      <element name="transparencyMetadata" type="tns:FileTransparencyMetadata" minOccurs="0">
        <annotation>
          <documentation>Information about the transparency metadata associated to the file</documentation>
        </annotation>
      </element>
      <element name="systemDocumentCount" type="xsd:int" minOccurs="0">
        <annotation>
          <documentation>Count of total documents filed in a given file</documentation>
        </annotation>
      </element>
      <element name="accessibleDocumentCount" type="xsd:int" minOccurs="0">
        <annotation>
          <documentation>Count of documents which are accessible to the user, in a given file</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="Heading">
    <annotation>
      <documentation>A heading (rubrique)</documentation>
    </annotation>
    <sequence>
      <element name="headingId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID to uniquely identify the heading</documentation>
        </annotation>
      </element>
      <element name="type" type="tns:HeadingType">
        <annotation>
          <documentation>The type of the heading (COMMON or SPECIFIC)</documentation>
        </annotation>
      </element>
      <element name="path" type="xsd:string">
        <annotation>
          <documentation>Logical path to the heading</documentation>
        </annotation>
      </element>
      <element name="headingCode" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>The heading's code</documentation>
        </annotation>
      </element>
      <element name="specificCode" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>The heading's specific code</documentation>
        </annotation>
      </element>
      <element name="status" type="tns:HeadingStatus" minOccurs="0">
        <annotation>
          <documentation>The heading's status</documentation>
        </annotation>
      </element>
      <element name="levelCode" type="xsd:string">
        <annotation>
          <documentation>The heading's level code</documentation>
        </annotation>
      </element>
      <element name="headingCodeStep" type="xsd:string">
        <annotation>
          <documentation>The heading's code step</documentation>
        </annotation>
      </element>
      <element name="parentId" type="tns:ObjectId" minOccurs="0">
        <annotation>
          <documentation>Internal repository ID of the parent heading or filing plan</documentation>
        </annotation>
      </element>
      <element name="creationDate" type="date">
        <annotation>
          <documentation>Date when the heading was created</documentation>
        </annotation>
      </element>
      <element name="englishName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the heading in English</documentation>
        </annotation>
      </element>
      <element name="frenchName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the heading in French</documentation>
        </annotation>
      </element>
      <element name="germanName" type="string" minOccurs="0">
        <annotation>
          <documentation>Name of the heading in German</documentation>
        </annotation>
      </element>
      <element name="serviceOwners" minOccurs="0">
        <annotation>
          <documentation>Collection of service owners</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="serviceOwner" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the service owner</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="headingReaders" minOccurs="0">
        <annotation>
          <documentation>Collection of heading readers</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="reader" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the heading reader</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="headingEditors" minOccurs="0">
        <annotation>
          <documentation>Collection of heading editors</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="editor" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the heading editor</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="fileCreators" minOccurs="0">
        <annotation>
          <documentation>Collection of file creators</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="creator" minOccurs="0" maxOccurs="unbounded" type="string">
              <annotation>
                <documentation>The user or group name of the file creators</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="englishDescription" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Description of the heading in English</documentation>
        </annotation>
      </element>
      <element name="frenchDescription" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Description of the heading in French</documentation>
        </annotation>
      </element>
      <element name="germanDescription" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Description of the heading in German</documentation>
        </annotation>
      </element>
      <element name="englishComments" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Comments of the heading in English</documentation>
        </annotation>
      </element>
      <element name="frenchComments" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Comments of the heading in French</documentation>
        </annotation>
      </element>
      <element name="germanComments" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>Description of the heading in German</documentation>
        </annotation>
      </element>
      <element name="categories" minOccurs="0">
        <annotation>
          <documentation>Categories linked to this heading.</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="category" maxOccurs="unbounded" type="tns:Category">
              <annotation>
                <documentation>The category</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="Category">
    <annotation>
      <documentation>A category</documentation>
    </annotation>
    <sequence>
      <element name="categoryId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID to uniquely identify the category</documentation>
        </annotation>
      </element>
      <element name="categoryKey" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID to identify the version tree of a category. For the first
                    version, the categoryId will be equal to the categoryKey.</documentation>
        </annotation>
      </element>
      <element name="code" type="xsd:string">
        <annotation>
          <documentation>The hierarchical code e.g. 1.5.2</documentation>
        </annotation>
      </element>
      <element name="englishName" type="xsd:string">
        <annotation>
          <documentation>The category name in English</documentation>
        </annotation>
      </element>
      <element name="frenchName" type="xsd:string">
        <annotation>
          <documentation>The category name in French</documentation>
        </annotation>
      </element>
      <element name="version" type="xsd:int">
        <annotation>
          <documentation>The version number. The first version is  version 1.</documentation>
        </annotation>
      </element>
      <element name="type" type="tns:CategoryType" minOccurs="0">
        <annotation>
          <documentation>Coordination / Contribution</documentation>
        </annotation>
      </element>
      <element name="status" type="tns:CategoryStatus">
        <annotation>
          <documentation>The category status</documentation>
        </annotation>
      </element>
      <element name="englishDescription" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>English description</documentation>
        </annotation>
      </element>
      <element name="frenchDescription" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>French description</documentation>
        </annotation>
      </element>
      <element name="englishObservations" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>English observations</documentation>
        </annotation>
      </element>
      <element name="frenchObservations" type="xsd:string" minOccurs="0">
        <annotation>
          <documentation>French observations</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="Document">
    <annotation>
      <documentation>A document</documentation>
    </annotation>
    <sequence>
      <element name="documentId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID of a document</documentation>
        </annotation>
      </element>
      <element name="title" type="string" minOccurs="0">
        <annotation>
          <documentation>Title/subject of the document</documentation>
        </annotation>
      </element>
      <element name="mailType" type="tns:MailType" minOccurs="0">
        <annotation>
          <documentation>Mail type</documentation>
        </annotation>
      </element>
      <element name="documentDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Date of the document</documentation>
        </annotation>
      </element>
      <element name="sentDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Date at which the document has been sent</documentation>
        </annotation>
      </element>
      <element name="encodingDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Date at which the document has been encoded in the
                        common repository</documentation>
        </annotation>
      </element>
      <element name="modificationDate" type="dateTime" minOccurs="0">
        <annotation>
          <documentation>Date+Time when the document was last modified. A
                        modification is considered to be any change to the document's
                        metadata, filing, items, senders or recipients.</documentation>
        </annotation>
      </element>
      <element name="registrationDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Date at which the document has been registered in
                        the common repository</documentation>
        </annotation>
      </element>
      <element name="saveNumber" type="string" minOccurs="0">
        <annotation>
          <documentation>Internal reference of a document</documentation>
        </annotation>
      </element>
      <element name="registrationNumber" type="string" minOccurs="0">
        <annotation>
          <documentation>Official reference of a document</documentation>
        </annotation>
      </element>
      <element name="saverEcasId" type="string" minOccurs="0">
        <annotation>
          <documentation>ECAS user name of the user that encoded the document
                        in the
                        common repository. If the user was acting on behalf of somebody else
                        (i.e. by using delegations in Ares), this field
                        contains the real user, not the on-behalf-of user (e.g. if a secretary
                        acts
                        on behalf of a head of unit, this field contains the ecas id of the
                        secretary).</documentation>
        </annotation>
      </element>
      <element name="registererEcasId" type="string" minOccurs="0">
        <annotation>
          <documentation>ECAS user name of the user that registered the
                        document in the
                        common repository. If the user was acting on behalf of somebody else,
                        this
                        field contains the real user, not the on-behalf-of user (e.g. if a
                        secretary acts
                        on behalf of a head of unit, this field contains the ecas id of the
                        secretary, not of the head of unit).</documentation>
        </annotation>
      </element>
      <element name="createdOnBehalfOf" type="string" minOccurs="0">
        <annotation>
          <documentation>User name of the user on whose behalf the document
                        was encoded in the
                        common repository. For a user that doesn't use a delegation in Ares when
                        creating the document, this
                        field will be the same as saverEcasId. For a user that acts on behalf
                        of somebody else,
                        the two fields will be different (e.g if a secretary acts in Ares
                        on behalf of a head of unit, this field will contain the ecas id of
                        the head of unit).</documentation>
        </annotation>
      </element>
      <element name="comments" type="string" minOccurs="0">
        <annotation>
          <documentation>Comments</documentation>
        </annotation>
      </element>
      <element name="securityClassification" type="tns:SecurityClassification" minOccurs="0">
        <annotation>
          <documentation>Security classification to be applied to the
                        document</documentation>
        </annotation>
      </element>
      <element name="markerType" type="tns:MarkerType" minOccurs="0">
        <annotation>
          <documentation>Marker to be applied to the document according to
                        the security classification</documentation>
        </annotation>
      </element>
      <element name="markerDeadline" type="date" minOccurs="0">
        <annotation>
          <documentation>Deadline associated to the marker. The marker will
                        expire at this date.</documentation>
        </annotation>
      </element>
      <element name="markerExpiryEvent" type="string" minOccurs="0">
        <annotation>
          <documentation>Marking expiry event. This is optional and is
                        treated as a comment field.</documentation>
        </annotation>
      </element>
      <element name="classificationDeadline" type="date" minOccurs="0">
        <annotation>
          <documentation>Deadline associated to the classification. The
                        classification will expire at this date.</documentation>
        </annotation>
      </element>
      <element name="classificationExpiryEvent" type="string" minOccurs="0">
        <annotation>
          <documentation>Classification expiration event. This is optional
                        and is treated as a comment field.</documentation>
        </annotation>
      </element>
      <element name="personConcerned" minOccurs="0">
        <annotation>
          <documentation>A recipient of the document to which this document
                        is addressed. This person
                        is considered the 'person concerned' and can only be specified using
                        a marker.
                        This is element is valid if there is a marker of type personal, staff
                        matter, medical matter and personal data.
                        It can be an internal or external person entity, not an organization
                        entity. It should not be a virtual entity.</documentation>
        </annotation>
        <complexType>
          <sequence>
            <choice>
              <element name="internalEntity" type="tns:InternalEntity">
                <annotation>
                  <documentation>Internal entity</documentation>
                </annotation>
              </element>
              <element name="externalEntity" type="tns:ExternalEntity">
                <annotation>
                  <documentation>External entity</documentation>
                </annotation>
              </element>
            </choice>
          </sequence>
        </complexType>
      </element>
      <element name="markerService" type="string" minOccurs="0">
        <annotation>
          <documentation>The service specified when the LIMITED_SERVICE
                        marking was
                        applied (may be missing for older documents).</documentation>
        </annotation>
      </element>
      <element name="encryptItems" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates whether the content of the items is stored encrypted in
                        the repository. In case the content is stored encrypted, the
                        decryption will be done automatically when downloading the
                        content.</documentation>
        </annotation>
      </element>
      <element name="encryptItemsDeadline" type="date" minOccurs="0">
        <annotation>
          <documentation>Date when the encryption of the document's items
                        expires.</documentation>
        </annotation>
      </element>
      <element name="frozen" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates if the document is frozen.
                        A frozen document cannot be updated any more (even if it's not
                        registered).
                        A document becomes frozen when it was filed in a file that has been
                        closed.</documentation>
        </annotation>
      </element>
      <element name="noteToTheFile" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates if the document is a "note to the file" /
                        "note au dossier"</documentation>
        </annotation>
      </element>
      <element name="senders" minOccurs="0">
        <annotation>
          <documentation>List of senders</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="sender" minOccurs="0" maxOccurs="unbounded">
              <complexType>
                <sequence>
                  <choice>
                    <element name="internalEntity" type="tns:InternalEntity">
                      <annotation>
                        <documentation>Internal entity</documentation>
                      </annotation>
                    </element>
                    <element name="externalEntity" type="tns:ExternalEntity">
                      <annotation>
                        <documentation>External entity</documentation>
                      </annotation>
                    </element>
                  </choice>
                  <element name="validForRegistration" type="boolean" minOccurs="0">
                    <annotation>
                      <documentation>Flag that indicates if the sender is valid in order the document
                                                can be registered.
                                                Internal entities can become invalid if the person changed
                                                service/DG or left the
                                                EC between the document was created and the time of
                                                validation/registration.
                                                External entities can be updated or deleted.</documentation>
                    </annotation>
                  </element>
                </sequence>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="recipients" minOccurs="0">
        <annotation>
          <documentation>List of recipients</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="recipient" minOccurs="0" maxOccurs="unbounded">
              <complexType>
                <sequence>
                  <choice>
                    <element name="internalEntity" type="tns:InternalEntity">
                      <annotation>
                        <documentation>Internal entity</documentation>
                      </annotation>
                    </element>
                    <element name="externalEntity" type="tns:ExternalEntity">
                      <annotation>
                        <documentation>External entity</documentation>
                      </annotation>
                    </element>
                  </choice>
                  <element name="code" type="tns:RecipientCode">
                    <annotation>
                      <documentation>The recipient code</documentation>
                    </annotation>
                  </element>
                  <element name="validForRegistration" type="boolean" minOccurs="0">
                    <annotation>
                      <documentation>Flag that indicates if the recipient is valid in order the
                                                document can be registered.
                                                Internal entities can become invalid if the person changed
                                                service/DG or left the
                                                EC between the document was created and the time of
                                                validation/registration.
                                                External entities can be updated or deleted.</documentation>
                    </annotation>
                  </element>
                </sequence>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="items" minOccurs="0">
        <annotation>
          <documentation>List of attachments</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="item" type="tns:Item" minOccurs="0" maxOccurs="unbounded">
              <annotation>
                <documentation>An attachment (the most recent version)</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="filedIn" minOccurs="0">
        <annotation>
          <documentation>List of files where the document has been filed</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="file" minOccurs="0" maxOccurs="unbounded">
              <complexType>
                <sequence>
                  <element name="fileId" type="tns:ObjectId">
                    <annotation>
                      <documentation>Internal repository ID to uniquely identify
                                                the file</documentation>
                    </annotation>
                  </element>
                  <element name="path" type="xsd:string">
                    <annotation>
                      <documentation>Logical path to the file</documentation>
                    </annotation>
                  </element>
                  <element name="chefDeFile" type="xsd:string" minOccurs="0">
                    <annotation>
                      <documentation>Name of 'main lead department' / 'chef de
                                                file'</documentation>
                    </annotation>
                  </element>
                  <element name="fileCode" type="xsd:string" minOccurs="0">
                    <annotation>
                      <documentation>The file's code</documentation>
                    </annotation>
                  </element>
                  <element name="specificCode" type="xsd:string" minOccurs="0">
                    <annotation>
                      <documentation>The file's specific code</documentation>
                    </annotation>
                  </element>
                  <element name="status" type="tns:FileStatus" minOccurs="0">
                    <annotation>
                      <documentation>The file's status</documentation>
                    </annotation>
                  </element>
                  <element name="englishName" type="string" minOccurs="0">
                    <annotation>
                      <documentation>Name of the file in English</documentation>
                    </annotation>
                  </element>
                  <element name="frenchName" type="string" minOccurs="0">
                    <annotation>
                      <documentation>Name of the file in French</documentation>
                    </annotation>
                  </element>
                  <element name="germanName" type="string" minOccurs="0">
                    <annotation>
                      <documentation>Name of the file in German</documentation>
                    </annotation>
                  </element>
                  <element name="filingDate" type="xsd:dateTime" minOccurs="0">
                    <annotation>
                      <documentation>Date the document was filed in the file</documentation>
                    </annotation>
                  </element>
                </sequence>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="links" minOccurs="0">
        <annotation>
          <documentation>List of linked documents</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="link" type="tns:Link" minOccurs="0" maxOccurs="unbounded">
              <annotation>
                <documentation>A link</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="assignmentWorkflow" minOccurs="0" type="tns:AssignmentWorkflow">
        <annotation>
          <documentation>Assignment workflow assigned to the document</documentation>
        </annotation>
      </element>
      <element name="signatoryWorkflow" minOccurs="0" type="tns:SignatoryWorkflow">
        <annotation>
          <documentation>Paper Signatory or e-Signatory workflow assigned to
                        the document</documentation>
        </annotation>
      </element>
      <element name="supportingItems" type="tns:SupportingItems" minOccurs="0">
        <annotation>
          <documentation>Wrapper element for listing all supporting Items associated with document's e-signatory workflow</documentation>
        </annotation>
      </element>
      <element name="procedure" minOccurs="0" type="tns:Procedure">
        <annotation>
          <documentation>Procedure linked to the document</documentation>
        </annotation>
      </element>
      <xsd:element name="signedBy" minOccurs="0" maxOccurs="unbounded">
        <annotation>
          <documentation>A list of users that have 'signed' a registered document (i.e.
                        they had a SIGN task in the eSignatory workflow and have closed it).
                        The list is in reverse order compared to the eSignatory order
                        (i.e. the last
                        user who signed is first in the list).
                        For an unregistered document, the list is empty even if some users
                        have
                        already closed their SIGN tasks.</documentation>
        </annotation>
        <xsd:complexType>
          <sequence>
            <xsd:element name="user" type="tns:WorkflowUser">
              <annotation>
                <documentation>The user that signed</documentation>
              </annotation>
            </xsd:element>
            <xsd:element name="on" type="xsd:date">
              <annotation>
                <documentation>The date when the user signed</documentation>
              </annotation>
            </xsd:element>
          </sequence>
        </xsd:complexType>
      </xsd:element>
      <element name="externalizationDetails" type="tns:ExternalizationDetails" minOccurs="0">
        <annotation>
          <documentation>Contains details about the externalization process of the document. This
                        element is present
                        only if requested through the corresponding document retrieval option.</documentation>
        </annotation>
      </element>
      <element name="specificMetadata" type="tns:SpecificMetadata" minOccurs="0">
        <annotation>
          <documentation>Contains (business) specific metadata added by client applications. This
                        metadata
                        will only be fetched when explicitly requested using a retrieval
                        option.</documentation>
        </annotation>
      </element>
      <element name="signAndLock" type="xsd:boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates that the action 'sign and lock' was used on the document. The
                        attachments
                        cannot be modified anymore (with some exceptions such as translations
                        and courtesy copy)</documentation>
        </annotation>
      </element>
      <element name="partitionId" type="int" minOccurs="0">
        <annotation>
          <documentation>Partition ID which document belongs to.
                        Each document should be associated with one partition.</documentation>
        </annotation>
      </element>
      <element name="specificMetadataItems" minOccurs="0">
        <annotation>
          <documentation>List of business specific metadata items/attachments</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="specificMetadataItem" type="tns:SpecificMetadataItem" minOccurs="1" maxOccurs="10">
              <annotation>
                <documentation>A specific metadata item/attachment</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="externalSignatures" minOccurs="0">
        <annotation>
          <documentation>List of external signatures</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="externalSignature" type="tns:ExternalSignature" minOccurs="1" maxOccurs="unbounded">
              <annotation>
                <documentation>An external signature</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="accessibilityInfo" type="tns:AccessibilityInfo" minOccurs="0">
        <annotation>
          <documentation>Information regarding the accessibility of the document
                        (if requested using the dedicated retrieval option).</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="Workflow" abstract="true">
    <annotation>
      <documentation>A workflow</documentation>
    </annotation>
    <sequence>
      <element name="workflowId" type="int">
        <annotation>
          <documentation>Internal ID to uniquely identify a workflow</documentation>
        </annotation>
      </element>
      <element name="documentId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID of the document for which this workflow exists</documentation>
        </annotation>
      </element>
      <element name="active" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates if the workflow is currently active. A workflow is considered active if it has active/launched tasks.</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="AssignmentWorkflow">
    <annotation>
      <documentation>A workflow of type Assignment</documentation>
    </annotation>
    <complexContent>
      <extension base="tns:Workflow">
        <sequence>
          <element name="tasks" minOccurs="0">
            <annotation>
              <documentation>The collection of tasks of the Assignment workflow</documentation>
            </annotation>
            <complexType>
              <sequence>
                <element name="task" type="tns:AssignmentTask" minOccurs="0" maxOccurs="unbounded">
                  <annotation>
                    <documentation>An Assignment workflow task</documentation>
                  </annotation>
                </element>
              </sequence>
            </complexType>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="SignatoryWorkflow">
    <annotation>
      <documentation>A workflow of type Signatory, can be a Paper or e-Signatory workflow</documentation>
    </annotation>
    <complexContent>
      <extension base="tns:Workflow">
        <sequence>
          <element name="managers" type="tns:WorkflowManagers">
            <annotation>
              <documentation>Managers involved in the workflow</documentation>
            </annotation>
          </element>
          <element name="launcher" type="tns:WorkflowUser" minOccurs="0">
            <annotation>
              <documentation>User that launched the workflow</documentation>
            </annotation>
          </element>
          <element name="launchDate" type="date" minOccurs="0">
            <annotation>
              <documentation>Day when the workflow has been launched</documentation>
            </annotation>
          </element>
          <element name="signatoryType" type="tns:SignatoryType">
            <annotation>
              <documentation>Type of workflow (paper or eSignatory)</documentation>
            </annotation>
          </element>
          <xsd:element name="lastESignatoryTaskId" type="xsd:int" minOccurs="0">
            <annotation>
              <documentation>Id of the last eSignatory task in the workflow. The field is present only if the
                                workflow type is eSignatory.</documentation>
            </annotation>
          </xsd:element>
          <element name="tasks" minOccurs="0">
            <annotation>
              <documentation>The collection of tasks of the Signatory workflow</documentation>
            </annotation>
            <complexType>
              <sequence>
                <element name="task" type="tns:SignatoryTask" minOccurs="0" maxOccurs="unbounded">
                  <annotation>
                    <documentation>An Signatory workflow task</documentation>
                  </annotation>
                </element>
              </sequence>
            </complexType>
          </element>
          <xsd:element name="parallelPaperSignatory" type="xsd:boolean">
            <annotation>
              <xsd:documentation>TRUE if there is a parallel paper signatory.</xsd:documentation>
            </annotation>
          </xsd:element>
          <element name="afterRegistrationAccess" type="tns:AfterRegistrationAccess"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
