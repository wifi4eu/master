        <annotation>
          <documentation>The user's full name (or a part of it)</documentation>
        </annotation>
      </element>
      <element name="organizationCode" type="string">
        <annotation>
          <documentation>Organization to which the user belongs</documentation>
        </annotation>
      </element>
      <element name="phone" type="string" minOccurs="0">
        <annotation>
          <documentation>Telephone number to contact the user</documentation>
        </annotation>
      </element>
      <element name="address" type="string" minOccurs="0">
