                <enumeration value="CF (QP)"/>
                <enumeration value="CF (CIS)"/>
                <enumeration value="CF (CAB)"/>
                <enumeration value="CF (PET)"/>
                <enumeration value="CONTR"/>
                <enumeration value="VALID"/>
                <enumeration value="EMET"/>
                <enumeration value="RET"/>
