  <complexType name="Task" abstract="true">
    <annotation>
      <documentation>A workflow task</documentation>
    </annotation>
    <sequence>
      <element name="taskId" type="int">
        <annotation>
          <documentation>Internal ID to uniquely identify a workflow task</documentation>
        </annotation>
      </element>
      <element name="workflowId" type="int">
        <annotation>
          <documentation>Internal ID of the workflow to which the task belongs</documentation>
        </annotation>
      </element>
      <element name="status">
        <annotation>
          <documentation>Current state of the workflow task</documentation>
        </annotation>
        <simpleType>
          <restriction base="xsd:string">
            <enumeration value="DRAFT"/>
            <enumeration value="LAUNCHED"/>
            <enumeration value="ACTIVE"/>
            <enumeration value="CLOSED"/>
            <enumeration value="BYPASSED"/>
            <enumeration value="MANUAL"/>
            <enumeration value="DECLINED"/>
            <enumeration value="DELEGATED"/>
            <enumeration value="RETURNED_TO_SENDER"/>
          </restriction>
        </simpleType>
      </element>
      <element name="assignee" type="tns:WorkflowUser">
        <annotation>
          <documentation>User to whom the task is assigned</documentation>
        </annotation>
      </element>
      <element name="sender" type="tns:WorkflowUser">
        <annotation>
          <documentation>User that sent the task to the assignee</documentation>
        </annotation>
      </element>
      <element name="sentDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Day on which the task was sent to the assignee</documentation>
        </annotation>
      </element>
      <element name="closer" type="tns:WorkflowUser" minOccurs="0">
        <annotation>
          <documentation>User that closed the task</documentation>
        </annotation>
      </element>
      <element name="closeDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Day on which the task was closed</documentation>
        </annotation>
      </element>
      <element name="reader" type="tns:WorkflowUser" minOccurs="0">
        <annotation>
          <documentation>User that has read the task first</documentation>
        </annotation>
      </element>
      <element name="readDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Day on which the task was first read</documentation>
        </annotation>
      </element>
      <element name="deadlineDate" type="date" minOccurs="0">
        <annotation>
          <documentation>Deadline day for this task, it should be closed by then</documentation>
        </annotation>
      </element>
      <element name="instructions" type="string" minOccurs="0">
        <annotation>
          <documentation>Instructions given by the sender of the task</documentation>
        </annotation>
      </element>
      <element name="comments" type="string" minOccurs="0">
        <annotation>
          <documentation>Comments given by the user that closed the task</documentation>
        </annotation>
      </element>
      <element name="critical" type="boolean">
        <annotation>
          <documentation>Indicates if the task is critical</documentation>
        </annotation>
      </element>
      <element name="deleted" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates that the task has been marked as deleted</documentation>
        </annotation>
      </element>
