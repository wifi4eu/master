  <complexType name="Item">
    <annotation>
      <documentation>An item/attachment</documentation>
    </annotation>
    <sequence>
      <element name="itemId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID to uniquely identify an item</documentation>
        </annotation>
      </element>
      <element name="name" type="string">
        <annotation>
          <documentation>The item's name</documentation>
        </annotation>
      </element>
      <element name="language" type="tns:LanguageCode">
        <annotation>
          <documentation>The item's language</documentation>
        </annotation>
      </element>
      <element name="attachmentType" type="tns:AttachmentType">
        <annotation>
          <documentation>Type of the item such as NATIVE_ELECTRONIC, ARES_SCANNED etc.</documentation>
        </annotation>
      </element>
      <element name="kind" type="tns:ItemKind">
        <annotation>
          <documentation>Kind of item such as MAIN, ANNEX, TRANSLATION etc.</documentation>
        </annotation>
      </element>
      <element name="isScanWithoutContent" type="boolean">
        <annotation>
          <documentation>Is true for items of type ARES_SCANNED (i.e. scans) where the content hasn't been scanned yet. Is false in all other cases.</documentation>
        </annotation>
      </element>
      <element name="modificationDate" type="dateTime">
        <annotation>
          <documentation>Date+Time when the item was last modified. A modification is considered to be any change to the item's metadata or to the item's content such as a checkin or scan operation.</documentation>
        </annotation>
      </element>
      <element name="externalReference" type="string" minOccurs="0">
        <annotation>
          <documentation>External reference associated with the item</documentation>
        </annotation>
      </element>
      <element name="contentType" type="string" minOccurs="0">
        <annotation>
          <documentation>Content type of item/attachment content (e.g. pdf, doc, msg, etc.)</documentation>
        </annotation>
      </element>
      <element name="pageNo" type="int" minOccurs="0">
        <annotation>
          <documentation>Number of pages that exist for the attachment's content</documentation>
        </annotation>
      </element>
      <element name="renditionStatus" type="tns:RenditionStatus" minOccurs="0">
        <annotation>
          <documentation>Status of the rendition of the item's content. In case the rendition is successful
                        you can download the rendition PDF. In case of failure, you can download an HTML
                        containing a description of the failure.</documentation>
        </annotation>
      </element>
      <element name="checkedOut" minOccurs="0">
        <annotation>
          <documentation>If the item is checked out, this element holds checkout
                        information</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="userName" type="string">
              <annotation>
                <documentation>Username that performed the checkout operation</documentation>
              </annotation>
            </element>
            <element name="date" type="dateTime">
              <annotation>
                <documentation>Date+Time when the checkout was performed</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="translations" minOccurs="0">
        <annotation>
          <documentation>This item's translations</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="translation" type="tns:Item" minOccurs="1" maxOccurs="unbounded">
              <annotation>
                <documentation>A translation is an uploaded item (i.e. type ARES_SCANNED
                                    is not valid for a translation), its item kind is
                                    always 'TRANSLATION', it cannot have other translations
                                    (i.e the 'translations' field is absent), and it cannot be
                                    checked out.</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element name="contentSize" type="int" minOccurs="0">
        <annotation>
          <documentation>Size of item/attachment content in bytes</documentation>
        </annotation>
      </element>
      <element name="versioned" type="boolean" minOccurs="0">
        <annotation>
          <documentation>Indicates if more than one version of this item exist. Using checkout /
                        checkin an item can be versioned and becomes part of a version tree.</documentation>
        </annotation>
      </element>
      <element name="versionLabel" type="string" minOccurs="0">
        <annotation>
          <documentation>The version label of this item. For example 1.0, 1.1, 2.0, ...</documentation>
        </annotation>
      </element>
      <element name="mimeType" type="string" minOccurs="0">
        <annotation>
          <documentation>Mime type of item/attachment content (e.g. text/html, application/vnd.ms-excel, etc.)</documentation>
        </annotation>
      </element>
      <element name="contentId" type="tns:ContentIdType" minOccurs="0">
        <annotation>
          <documentation>Content id that was obtained by uploading the content using
                        the data transfer service.
                        (Note: This value does not exist for items uploaded in the past and it will not exist for items uploaded using Ares, AresLook, ...)</documentation>
        </annotation>
      </element>
      <element name="sha256" type="string" minOccurs="0">
        <annotation>
          <documentation>The SHA-256 checksum in hexadecimal format.
                        Can be used for verification or as input for EU Login / ECAS XML signatures.
                        Does not exist for old items or items that might have been ingested through other sources than HRS.</documentation>
        </annotation>
      </element>
      <element name="searchSummary" type="string" minOccurs="0">
        <annotation>
          <documentation>Summary of this item and its content. Only present if there was a search
                        executed against the index server and summary was requested.</documentation>
        </annotation>
      </element>
      <element name="versions" minOccurs="0">
        <annotation>
          <documentation>Previous versions of an attachment</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="item" type="tns:Item" minOccurs="0" maxOccurs="unbounded">
              <annotation>
                <documentation>A previous version of an attachment</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="SpecificMetadataItem">
    <annotation>
      <documentation>An item/attachment</documentation>
    </annotation>
    <sequence>
      <element name="itemId" type="tns:ObjectId">
        <annotation>
          <documentation>Internal repository ID to uniquely identify a specific metadata attachment</documentation>
        </annotation>
      </element>
      <element name="namespace" type="xsd:anyURI">
        <annotation>
          <documentation>Serves as an identifier for the type specific metadata, e.g. identify all
                        specific metadata items added by a given application.</documentation>
        </annotation>
      </element>
      <element name="name" type="string">
        <annotation>
          <documentation>The item's name</documentation>
        </annotation>
      </element>
      <element name="contentSize" type="int">
        <annotation>
          <documentation>Size of item/attachment content in bytes</documentation>
        </annotation>
      </element>
      <element name="externalReference" type="string" minOccurs="0">
        <annotation>
          <documentation>External reference associated with the item</documentation>
        </annotation>
      </element>
      <element name="creationDate" type="dateTime">
        <annotation>
          <documentation>Date and time when the business specific metadata item/attachment was created.</documentation>
        </annotation>
      </element>
      <element name="modificationDate" type="dateTime">
        <annotation>
          <documentation>Date and time when the business specific metadata item/attachment was last modified.</documentation>
        </annotation>
      </element>
      <element name="creatorUserName" type="string">
        <annotation>
          <documentation>The user name of the user that created the item/attachment</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="AccessibilityInfo">
    <annotation>
      <documentation>A set of flags indicating how and where to access a document. Useful for the cases where user does not have access
                or lost access to the document.</documentation>
    </annotation>
    <sequence>
      <element name="accessible" type="boolean">
        <annotation>
          <documentation>If true the document is accessible by the user in Hermes.</documentation>
        </annotation>
      </element>
      <element name="inHermes" type="boolean">
        <annotation>
          <documentation>If true the document is still in Hermes but not necessarily accessible by the user. If not trashed, access
                        can be granted by contacting the author of the document or the DMO.
                        If false, the document cannot be obtained via Ares or HRS.</documentation>
        </annotation>
      </element>
      <element name="trashed" type="boolean">
        <annotation>
          <documentation>If true it indicates the document is trashed (but still in Hermes) and not accessible by any user.
                        Access can be granted by contacting the DMO (or directly Ares support). If not recovered from trash in
                        time, it will be completely deleted.</documentation>
        </annotation>
      </element>
      <element name="transferred" type="boolean">
        <annotation>
          <documentation>If true it indicates that (an instance of) the document was transferred to A-REP.</documentation>
        </annotation>
      </element>
      <element name="eliminated" type="boolean">
        <annotation>
          <documentation>If true it indicates that (an instance of) the document has been eliminated. If all instances of a
                        document are eliminated, it cannot be accessed any more.</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="ExternalSignature">
    <annotation>
      <documentation>An external signature of a document. This represents a sign action done
                outside of Hermes and, thus, not via Hermes e-Signatory workflow.
                From Hermes point of view this is just stored as information and does not have any
                (legal) value.</documentation>
    </annotation>
    <sequence>
      <element name="id" type="int">
        <annotation>
          <documentation>Internal ID of the external signature.</documentation>
        </annotation>
      </element>
      <element name="signedBy" type="tns:WorkflowUser">
        <annotation>
          <documentation>User that signed the external signature in an external application.</documentation>
        </annotation>
      </element>
      <element name="dateSigned" type="dateTime">
        <annotation>
          <documentation>Date (and time) on which the document was signed.</documentation>
        </annotation>
      </element>
      <element name="application" type="string">
        <annotation>
          <documentation>Name of the external application in which the document was signed.</documentation>
        </annotation>
      </element>
      <element name="ecasSignature" type="boolean">
        <annotation>
          <documentation>Indicates whether the external signature was an ECAS digital XML signature</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
